{
  "name": "history",
  "version": "1.0.0",
  "description": "CLI for History App",
  "main": "lib/hist.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "build:umd": "webpack src/hist.js dist/hist.js",
    "build:umd:min": "cross-env NODE_ENV=production webpack src/hist.js dist/hist.min.js",
    "clean": "rimraf lib dist coverage",
    "lint": "eslint src test examples",
    "test": "NODE_ENV=test DB_NAME=clip-test mocha --compilers js:babel-register --recursive",
    "test:cov": "NODE_ENV=test DB_NAME=clip-test babel-node ./node_modules/isparta/bin/isparta cover ./node_modules/mocha/bin/_mocha -- --recursive",
    "test:watch": "NODE_ENV=test DB_NAME=clip-test npm test -- --watch"
  },
  "bin": {
    "hist": "./lib/hist.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/baugarten/History-CLI.git"
  },
  "keywords": [],
  "author": "Ben Augarten",
  "license": "",
  "bugs": {
    "url": "https://github.com/baugarten/History-CLI/issues"
  },
  "homepage": "https://github.com/baugarten/History-CLI",
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "bluebird": "^3.4.1",
    "co": "^4.6.0",
    "co-prompt": "^1.0.0",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "superagent": "^2.1.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-watch": "^2.0.2",
    "chai": "^3.5.0",
    "cross-env": "^1.0.7",
    "eslint": "^2.5.3",
    "hist-server": "file:../hist-server",
    "isparta": "^4.0.0",
    "knex-cleaner": "^1.1.3",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2",
    "should": "^10.0.0",
    "superagent-mock": "^1.10.0",
    "webpack": "^1.12.14"
  }
}
